<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Future_submit_date</fullName>
    <active>true</active>
    <errorConditionFormula>Submitted_date__c  &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Submitted_date__c</errorDisplayField>
    <errorMessage>Submitted date must not be in the future</errorMessage>
</ValidationRule>
