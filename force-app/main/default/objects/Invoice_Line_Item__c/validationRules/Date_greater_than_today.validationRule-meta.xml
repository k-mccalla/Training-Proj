<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_greater_than_today</fullName>
    <active>true</active>
    <description>Date for line item must be in the past</description>
    <errorConditionFormula>Date__c &gt;  TODAY()</errorConditionFormula>
